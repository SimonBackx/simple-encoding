{
    "search.exclude": {
        "**/dist": true
    },
    "[typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[vue]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.codeLens": true,
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "eslint.format.enable": true,
    "eslint.validate": [
        "typescript",
        "javascript",
        "vue"
    ],
}